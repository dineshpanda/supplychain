<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All account histories
    </h1>

    
      <a href="/account_histories/new" class="btn btn-block btn-outline-secondary">
        Add a new account history
      </a>
    
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Owner
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @account_histories.each do |account_history| %>
      <tr>
        <!-- Display account_history.id start -->
        <td>
          <%= account_history.id %>
        </td>
        <!-- Display account_history.id end -->

        <!-- Display account_history.owner_id start -->
        <td>
          <%= account_history.owner.supplier_id %>
        </td>
        <!-- Display account_history.owner_id end -->

        <!-- Display account_history.created_at start -->
        <td>
          <%= time_ago_in_words(account_history.created_at) %> ago
        </td>
        <!-- Display account_history.created_at end -->

        <!-- Display account_history.updated_at start -->
        <td>
          <%= time_ago_in_words(account_history.updated_at) %> ago
        </td>
        <!-- Display account_history.updated_at end -->

        <td>
          <a href="/account_histories/<%= account_history.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
