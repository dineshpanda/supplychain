<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All suppliers
    </h1>

    
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @suppliers.each do |supplier| %>
      <tr>
        <!-- Display supplier.id start -->
        <td>
          <%= supplier.id %>
        </td>
        <!-- Display supplier.id end -->

        <!-- Display supplier.created_at start -->
        <td>
          <%= time_ago_in_words(supplier.created_at) %> ago
        </td>
        <!-- Display supplier.created_at end -->

        <!-- Display supplier.updated_at start -->
        <td>
          <%= time_ago_in_words(supplier.updated_at) %> ago
        </td>
        <!-- Display supplier.updated_at end -->

        <td>
          <a href="/suppliers/<%= supplier.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
